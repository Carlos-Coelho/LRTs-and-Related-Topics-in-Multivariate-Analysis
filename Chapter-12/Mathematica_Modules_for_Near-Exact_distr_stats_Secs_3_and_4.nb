        FontSize->16,
        FontColor->RGBColor[1, 0, 0]], 
      StyleBox[\( (*\(\(**\)\(\ \ \)\(Also\)\)\ modules\ are\ provided\ for\ \
the\ near - 
            exact\ c . f . ' 
              s\ \((characteristic\ functions)\)\ of\ the\ negative\ \
logarithms\ of\ all\ three\ l . r . t . \ statistics, \ 
          mainly\ because\ from\ these\ it\ is\ very\ easy\ to\ obtain\ the\ \
values\ for\ the\ moments\ of\ the\ corresponding\ l . r . t . \ 
              statistics, \ by\ computing\ these\ on\ 1*I, \ 2*I, \  ... , \ 
          in\ order\ to\ btain\ the\ first, \ second, \  ... , \ 
          non - central\ moments\ of\ the\ l . r . t . \ 
                statistic\ \ \ \ \ ***) \),
        FontSize->16,
        FontColor->RGBColor[1, 0, 0]], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{\($MaxExtraPrecision = 500;\), "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Base\ Module\ for\ the\ computation\ of\ the\ \
coefficients\ c_ {j, k}\ in\ the\ p . d . f . \ and\ c . d . f . \ 
                of\ the\ GIG\ \((Generalized\ Integer\ Gamma)\)\ and\ the\ \
GNIG\ \((Generalized\ Near - Integer\ Gamma)\)\ distributions\ \ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(Makec[r_, l_, p_] := 
          Module[{c}, \[IndentingNewLine]c = 
              Table[Table[1, {j, 1, Max[r]}], {i, 1, p}]; \n
            Table[c = 
                ReplacePart[
                  c, \((Product[\((l[\([j]\)] - 
                                l[\([i]\)])\)^\((\(-r[\([j]\)]\))\), {j, 1, 
                            i - 1}]*
                        Product[\((l[\([j]\)] - 
                                l[\([i]\)])\)^\((\(-r[\([j]\)]\))\), {j, 
                            i + 1, p}])\)/\(\((r[\([i]\)] - 1)\)!\), {i, 
                    r[\([i]\)]}], {i, 1, p}]; \n
            Table[Table[
                c = ReplacePart[c, 
                    Sum[\((\(\((r[\([i]\)] - k + j - 1)\)!\)*\((Sum[
                                    r[\([h]\)]/\((l[\([i]\)] - l[\([h]\)])\)^
                                        j, {h, 1, i - 1}] + 
                                  Sum[r[\([h]\)]/\((l[\([i]\)] - 
                                        l[\([h]\)])\)^j, {h, i + 1, 
                                      p}])\)*\(c[\([i]\)]\)[\([r[\([i]\)] - \
\((k - j)\)]\)])\)/\(\((r[\([i]\)] - k - 1)\)!\), {j, 1, k}]/k, {i, 
                      r[\([i]\)] - k}], {k, 1, r[\([i]\)] - 1}], {i, 1, 
                p}]; \[IndentingNewLine]c\[IndentingNewLine]]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ p . d . f . \ 
                of\ the\ GIG\ \(\((\)\(Generalized\ Integer\ Gamma\ \
distribution\)\(\ \ \)\)*) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(GIGpdf[r_, li_, z_] := 
          Module[{p, l}, 
            If[Count[r, _Integer] \[Equal] Length[r] && And @@ Positive[r] && 
                And @@ Positive[li] && 
                Length[r] == Length[li], \[IndentingNewLine]p = 
                Length[r]; \[IndentingNewLine]l = 
                Rationalize[li, 0]; \[IndentingNewLine]c = 
                Makec[r, l, p]; \[IndentingNewLine]Product[
                  l[\([j]\)]^r[\([j]\)], {j, 1, p}]*
                Sum[Exp[\(-l[\([j]\)]\)*z]*
                    Sum[\(c[\([j]\)]\)[\([k]\)]*z^\((k - 1)\), {k, 1, 
                        r[\([j]\)]}], {j, 1, 
                    p}]\[IndentingNewLine]]\[IndentingNewLine]]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ c . d . f . \ 
                of\ the\ GIG\ \(\((\)\(Generalized\ Integer\ Gamma\ \
distribution\)\(\ \ \)\)*) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(GIGcdf[r_, li_, z_] := 
          Module[{p, l, c}, 
            If[Count[r, _Integer] \[Equal] Length[r] && And @@ Positive[r] && 
                And @@ Positive[li] && 
                Length[r] == Length[li], \[IndentingNewLine]p = 
                Length[r]; \[IndentingNewLine]l = 
                Rationalize[li, 0]; \[IndentingNewLine]c = 
                Makec[r, l, p]; \[IndentingNewLine]1 - 
                Product[l[\([j]\)]^r[\([j]\)], {j, 1, p}]*
                  Sum[Exp[\(-l[\([j]\)]\)*z]*
                      Sum[\(c[\([j]\)]\)[\([k]\)]*\(\((k - 1)\)!\)*
                          Sum[z^i/\((\(i!\)*l[\([j]\)]^\((k - i)\))\), {i, 0, 
                              k - 1}], {k, 1, r[\([j]\)]}], {j, 1, 
                      p}]\[IndentingNewLine]]\[IndentingNewLine]]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ p . d . f . \ 
                of\ the\ GNIG\ \(\((\)\(Generalized\ Near - 
                  Integer\ Gamma\ distribution\)\(\ \ \)\)*) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(GNIGpdf[rj_, ri_, lji_, li_, z_] := 
          Module[{p, lj, l, r, c}, \[IndentingNewLine]If[
              Count[rj, _Integer] \[Equal] Length[rj] && And @@ Positive[rj] && 
                And @@ Positive[lji] && Length[rj] == Length[lji] && ri > 0 && 
                li > 0, \[IndentingNewLine]p = 
                Length[rj]; \[IndentingNewLine]lj = 
                Rationalize[lji, 0]; \[IndentingNewLine]l = 
                Rationalize[li, 0]; \[IndentingNewLine]r = 
                Rationalize[ri, 0]; \[IndentingNewLine]c = 
                Makec[rj, lj, p]; \[IndentingNewLine]Product[
                  lj[\([j]\)]^rj[\([j]\)], {j, 1, p}]*l^r*
                Sum[Exp[\(-lj[\([j]\)]\)*z]*
                    Sum[\(c[\([j]\)]\)[\([k]\)]*Gamma[k]/Gamma[k + r]*
                        z^\((k + r - 1)\)*
                        Hypergeometric1F1[r, 
                          k + r, \(-\((l - lj[\([j]\)])\)\)*z], {k, 1, 
                        rj[\([j]\)]}], {j, 1, 
                    p}]\[IndentingNewLine]]\[IndentingNewLine]]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ c . d . f . \ 
                of\ the\ GNIG\ \(\((\)\(Generalized\ Near - 
                  Integer\ Gamma\ distribution\)\(\ \ \)\)*) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(GNIGcdf[rj_, ri_, lji_, li_, z_] := 
          Module[{p, lj, l, r, c}, \[IndentingNewLine]If[
              Count[rj, _Integer] \[Equal] Length[rj] && And @@ Positive[rj] && 
                And @@ Positive[lji] && Length[rj] == Length[lji] && ri > 0 && 
                li > 0, \[IndentingNewLine]p = 
                Length[rj]; \[IndentingNewLine]lj = 
                Rationalize[lji, 0]; \[IndentingNewLine]l = 
                Rationalize[li, 0]; \[IndentingNewLine]r = 
                Rationalize[ri, 0]; \[IndentingNewLine]c = 
                Makec[rj, lj, p]; \[IndentingNewLine]l^r*z^r/Gamma[r + 1]*
                  Hypergeometric1F1[r, r + 1, \(-l\)*z] - 
                Product[lj[\([j]\)]^rj[\([j]\)], {j, 1, p}]*l^r*
                  Sum[Exp[\(-lj[\([j]\)]\)*z]*
                      Sum[\(c[\([j]\)]\)[\([k]\)]/lj[\([j]\)]^k*Gamma[k]*
                          Sum[z^\((r + i)\)*lj[\([j]\)]^i/Gamma[r + 1 + i]*
                              Hypergeometric1F1[r, 
                                r + 1 + i, \(-\((l - lj[\([j]\)])\)\)*z], {i, 
                              0, k - 1}], {k, 1, rj[\([j]\)]}], {j, 1, 
                      p}]\[IndentingNewLine]]\[IndentingNewLine]]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ c . f . \ 
                of\ a\ general\ mixture\ of\ Gamma\ distributions\ \ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(FCMG[p_, r_, m_, t_] := 
          Module[{}, \[IndentingNewLine]c = 
              Length[p]; \[IndentingNewLine]pp = 
              Sum[p[\([\)\(j\)\(]\)], {j, 1, c}]; \[IndentingNewLine]Sum[
                p[\([\)\(j\)\(]\)]*
                  m[\([\)\(j\)\(]\)]^
                    r[\([\)\(j\)\(]\)]*\((m[\([\)\(j\)\(]\)] - 
                        I*t)\)^\((\(-r[\([\)\(j\)\(]\)]\))\), {j, 1, 
                  c}] + \((1 - pp)\)*
                m[\([\)\(1 + c\)\(]\)]^
                  r[\([\)\(1 + c\)\(]\)]*\((m[\([\)\(1 + c\)\(]\)] - 
                      I*t)\)^\((\(-r[\([\)\(1 + 
                          c\)\(]\)]\))\)\[IndentingNewLine]]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ computation\ of\ the\ moments\ of\
\ a\ general\ mixture\ of\ Gamma\ distributions\ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(MomMG[p_, r_, m_, h_] := 
          Module[{}, \[IndentingNewLine]c = 
              Length[p]; \[IndentingNewLine]pp = 
              Sum[p[\([\)\(j\)\(]\)], {j, 1, c}]; \[IndentingNewLine]Sum[
                p[\([\)\(j\)\(]\)]/m[\([\)\(j\)\(]\)]^h*
                  Product[\((r[\([\)\(j\)\(]\)] + i)\), {i, 0, h - 1}], {j, 
                  1, c}] + \((1 - pp)\)/m[\([\)\(1 + c\)\(]\)]^h*
                Product[\((r[\([\)\(1 + c\)\(]\)] + i)\), {i, 0, 
                    h - 1}]\[IndentingNewLine]]\), "\[IndentingNewLine]", 
        StyleBox["\[IndentingNewLine]",
          FontSize->14,
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\(\(**\)\(**\)\(\ \ \ \)\(Modules\)\)\ for\ the\ c . 
                f . \ and\ moments\ of\ the\ negative\ logarithm\ of\ the\ l \
. r . t . \ statistic\ for\ the\ sphericity\ test\ \ \ \ *****) \),
          FontSize->14,
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        StyleBox[\( (*\(\(\(\(\(\(\(\(**\)\(**\)\(\ \ \ \)\(All\)\)\ modules\ \
have\ three\ mandatory\ arguments\ \((in\ this\ order)\) : \ 
                          i\)\()\)\)\ the\ sample\ size, \ 
                    ii\)\()\)\)\ the\ number\ of\ variables, \ 
                iii\)\()\)\)\ the\ running\ variable\ for\ the\ function\ \ \ \
*****) \),
          FontSize->14,
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ part\ of\ the\ exact\ c . 
                f . \ \((characteristic\ function)\)\ of\ the\ negative\ \
logarithm\ of\ the\ l . r . 
                t . \ \((likelihood\ ratio\ test)\)\ statistic\ for\ the\ \
sphericity\ test\ which\ is\ left\ unchanged\ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(CFW4P1[n_, p_, t_] := 
          Product[\((\((n - j - 1)\)/n)\)^\((p - j)\)*\((\((n - j - 1)\)/n - 
                    I*t)\)^\((\(-\((p - j)\)\))\), {j, 1, p - 1}]\), 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ part\ of\ the\ exact\ c . 
                f . \ \((characteristic\ function)\)\ of\ the\ negative\ \
logarithm\ of\ the\ l . r . 
                t . \ \((likelihood\ ratio\ test)\)\ statistic\ for\ the\ \
sphericity\ test\ which\ is\ to\ be\ asymptotically\ approximated\ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(CFW4P2[n_, p_, t_] := 
          Product[Gamma[n - 1 + j/p]/Gamma[n - 1]*
              Gamma[n - 1 - n*I*t]/Gamma[n - 1 + j/p - n*I*t], {j, 1, 
              p - 1}]\), "\[IndentingNewLine]", 
        StyleBox["\[IndentingNewLine]",
          FontColor->RGBColor[0, 0, 1]], 
        StyleBox[\( (*\ \ Module\ for\ the\ whole\ exact\ c . 
                f . \ \((characteristic\ function)\)\ of\ the\ negative\ \
logarithm\ of\ the\ l . r . 
                t . \ \((likelihood\ ratio\ test)\)\ statistic\ for\ the\ \
sphericity\ test\ \ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(CFW4[n_, p_, t_] := 
          CFW4P1[n, p, t]*CFW4P2[n, p, t]\), "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ computation\ of\ the\ moments\ \
corresponding\ to\ the\ part\ of\ the\ c . 
                f . \ \((characteristic\ function)\)\ CFW4P2\ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(MomW4P2[n_, p_, h_] := 
          D[I^\((\(-h\))\)*CFW4P2[n, p, t], {t, h}] /. t \[Rule] 0\), 
        StyleBox["\[IndentingNewLine]",
          FontSize->14,
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        StyleBox["\[IndentingNewLine]",
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        StyleBox[\( (*\(\(**\)\(**\)\(\ \ \ \ \)\(Modules\)\)\ for\ the\ near \
- exact\ distributions\ for\ the\ l . r . t . \ 
                  statistic\ for\ the\ test\ of\ sphericity\ \ \ \ *****) \),
          FontSize->14,
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        StyleBox[\( (*\(\(\(\(\(\(\(\(\(\(**\)\(**\)\(\ \ \ \ \)\(All\)\)\ \
modules\ have\ as\ mandatory\ arguments\ \((in\ this\ order)\) : \ 
                              i\)\()\)\)\ the\ number\ of\ variables, \ 
                        ii\)\()\)\)\ the\ sample\ size, \ 
                    iii\)\()\)\)\ the\ number\ of\ exact\ moments\ to\ be\ \
matched, \ 
                iv\)\()\)\)\ the\ running\ variable\ for\ the\ function\ and\ \
a\ fifth, \ optional, \ 
            argument\ which\ is\ the\ number\ of\ precision\ digits\ used\ to\
\ compute\ the\ exact\ moments\ to\ be\ matched\ \((by\ default\ this\ value\ \
is\ 100)\)\ \ \ *****) \),
          FontSize->14,
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        StyleBox["\[IndentingNewLine]",
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ computation/
                  implementation\ of\ the\ c . f . \ of\ the\ near - 
              exact\ distribution\ for\ W4, \ 
            the\ negative\ logarithm\ of\ the\ l . r . t . \ 
                statistic\ for\ the\ sphericity\ test\ \ \ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(NECFW4[p_, n_, m_, t_, prec_:  100] := 
          Module[{mom, r, lamb, ppp}, \[IndentingNewLine]mom = 
              Table[SetPrecision[MomW4P2[n, p, h], prec], {h, 1, 
                  m}]; \[IndentingNewLine]Clear[
              pe]; \[IndentingNewLine]r = \((p - 1)\)/
                2; \[IndentingNewLine]lamb = \((n - 1)\)/
                n; \[IndentingNewLine]Off[
              Part::partd]; \[IndentingNewLine]ppp = 
              Flatten[Table[
                    pe[\([\)\(j\)\(]\)], {j, 1, 
                      m}] /. \[IndentingNewLine]NSolve[
                    Table[mom[\([\)\(h\)\(]\)] \[Equal] 
                        MomMG[Table[pe[\([\)\(j\)\(]\)], {j, 1, m}], 
                          Table[r + i, {i, 0, m}], 
                          Table[lamb, {i, 1, m + 1}], h], {h, 1, m}], 
                    Table[pe[\([\)\(j\)\(]\)], {j, 1, 
                        m}]]]; \[IndentingNewLine]CFW4P1[n, p, t]*
              FCMG[ppp, Table[r + i, {i, 0, m}], Table[lamb, {i, 1, m + 1}], 
                t]\[IndentingNewLine]]\), "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ computation/
                  implementation\ of\ the\ p . d . f . \ of\ the\ near - 
              exact\ distribution\ for\ the\ negative\ logarithm\ of\ the\ l \
. r . t . \ statistic\ for\ the\ sphericity\ test\ \ \ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(NEPDFW4[p_, n_, m_, z_, prec_:  100] := 
          Module[{mom, r, lamb, ppp}, \[IndentingNewLine]mom = 
              Table[
                SetPrecision[MomW4P2[n, p, h], prec], {h, 1, 
                  m}]; \[IndentingNewLine]Clear[
              pe]; \[IndentingNewLine]r = \((p - 1)\)/
                2; \[IndentingNewLine]lamb = \((n - 1)\)/
                n; \[IndentingNewLine]Off[
              Part::partd]; \[IndentingNewLine]ppp = 
              Flatten[Table[
                    pe[\([\)\(j\)\(]\)], {j, 1, 
                      m}] /. \[IndentingNewLine]NSolve[
                    Table[mom[\([\)\(h\)\(]\)] \[Equal] 
                        MomMG[Table[pe[\([\)\(j\)\(]\)], {j, 1, m}], 
                          Table[r + i, {i, 0, m}], 
                          Table[lamb, {i, 1, m + 1}], h], {h, 1, m}], 
                    Table[pe[\([\)\(j\)\(]\)], {j, 1, 
                        m}]]]; \[IndentingNewLine]tp = 
              Total[ppp]; \[IndentingNewLine]rj = 
              Table[p - j, {j, 1, p - 1}]; \[IndentingNewLine]lj = 
              Table[\((n - j)\)/n, {j, 2, p}]; \[IndentingNewLine]If[
              Mod[p, 2] \[Equal] 0, 
              Sum[ppp[\([j]\)]*GNIGpdf[rj, r + j - 1, lj, lamb, z], {j, 1, 
                    m}] + \((1 - tp)\)*GNIGpdf[rj, r + m, lj, lamb, z], 
              Sum[ppp[\([j]\)]*
                    GIGpdf[Flatten[{rj, {r + j - 1}}], Flatten[{lj, {lamb}}], 
                      z], {j, 1, m}] + \((1 - tp)\)*
                  GIGpdf[Flatten[{rj, {r + m}}], Flatten[{lj, {lamb}}], 
                    z]]\[IndentingNewLine]]\), "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ computation/
                  implementation\ of\ the\ c . d . f . \ of\ the\ near - 
              exact\ distribution\ for\ the\ negative\ logarithm\ of\ the\ l \
. r . t . \ statistic\ for\ the\ sphericity\ test\ \ \ \ *) \),
          FontColor->RGBColor[0, 0, 1]], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", \(NECDFW4[p_, n_, m_, z_, prec_:  100] := 
          Module[{mom, r, lamb, ppp}, \[IndentingNewLine]mom = 
              Table[SetPrecision[MomW4P2[n, p, h], prec], {h, 1, 
                  m}]; \[IndentingNewLine]Clear[
              pe]; \[IndentingNewLine]r = \((p - 1)\)/
                2; \[IndentingNewLine]lamb = \((n - 1)\)/
                n; \[IndentingNewLine]Off[
              Part::partd]; \[IndentingNewLine]ppp = 
              Flatten[Table[
                    pe[\([\)\(j\)\(]\)], {j, 1, 
                      m}] /. \[IndentingNewLine]NSolve[
                    Table[mom[\([\)\(h\)\(]\)] \[Equal] 
                        MomMG[Table[pe[\([\)\(j\)\(]\)], {j, 1, m}], 
                          Table[r + i, {i, 0, m}], 
                          Table[lamb, {i, 1, m + 1}], h], {h, 1, m}], 
                    Table[
                      pe[\([\)\(j\)\(]\)], {j, 1, 
                        m}]]]; \[IndentingNewLine]tp = 
              Total[ppp]; \[IndentingNewLine]rj = 
              Table[p - j, {j, 1, p - 1}]; \[IndentingNewLine]lj = 
              Table[\((n - j)\)/n, {j, 2, p}]; \[IndentingNewLine]If[
              Mod[p, 2] \[Equal] 0, 
              Sum[ppp[\([j]\)]*GNIGcdf[rj, r + j - 1, lj, lamb, z], {j, 1, 
                    m}] + \((1 - tp)\)*GNIGcdf[rj, r + m, lj, lamb, z], 
              Sum[ppp[\([j]\)]*
                    GIGcdf[Flatten[{rj, {r + j - 1}}], Flatten[{lj, {lamb}}], 
                      z], {j, 1, m}] + \((1 - tp)\)*
                  GIGcdf[Flatten[{rj, {r + m}}], Flatten[{lj, {lamb}}], 
                    z]]\[IndentingNewLine]]\), "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        StyleBox[\( (*\ \ Module\ for\ the\ computation/
                  implementation\ of\ the\ p . d . f . \ of\ the\ near - 
              exact\ distribution\ for\ the\ l . r . t . \ 
                  statistic\ for\ the\ sphericity\ test, \ 
            Lambda_  4\ \((L4)\)\ \ \ \ *) \),
